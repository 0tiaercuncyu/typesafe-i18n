import { initFormatters } from './formatters-template.actual.output'

export const locales: Locales[] = [
	'de-at'
]

const localeTranslationLoaders = {
	'de-at': () => import('./de-at'),
}

export const getTranslationForLocale = async (locale: Locales) => (await (localeTranslationLoaders[locale] || localeTranslationLoaders['de-at'])()).default as Translation

